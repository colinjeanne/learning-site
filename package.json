{
  "name": "learning-site",
  "version": "1.0.0",
  "description": "A simple site for tracking children's educational materials",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build:stylesheets": "ncp assets/stylesheets/ public/",
    "clean": "rimraf ./build",
    "generate:data": "node ./tools/assets/generate-data.js",
    "prepare": "npm run clean & npm run build:stylesheets & gulp prepare",
    "test": "npm run clean & gulp test"
  },
  "keywords": [
    "education",
    "learning"
  ],
  "author": "Colin Jeanne <colinjeanne@hotmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "browserify": "^13.0.0",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-jasmine": "^2.3.0",
    "gulp-uglify": "^1.5.3",
    "ncp": "^2.0.0",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.0.0",
    "redux": "^3.0.0",
    "redux-actions": "^0.9.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.2",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
